APP_APP_NAME=china-job-aggregator
APP_ENV=dev
APP_DEBUG=true
APP_API_PREFIX=/api/v1
APP_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/jobdb
APP_LOG_LEVEL=INFO
APP_CORS_ORIGINS=["http://localhost:3000"]
APP_SCHEDULER_ENABLED=true
APP_SCHEDULER_TIMEZONE=Asia/Shanghai
APP_SCHEDULER_DEFAULT_INTERVAL_MINUTES=30
APP_CRAWLER_DEFAULT_TIMEOUT_SECONDS=20
APP_CRAWLER_DEFAULT_RETRY_COUNT=3
APP_CRAWLER_DEFAULT_BACKOFF_SECONDS=1.0
APP_SITES_CONFIG_PATH=configs/sites.yaml
